# Supabase Configuration
SUPABASE_URL=https://supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Gemini / Google GenAI API key (set one)
GOOGLE_API_KEY=
GEMINI_API_KEY=

# Supabase Storage Bucket for IMU data
SUPABASE_IMU_BUCKET=imu-data
SUPABASE_DATAPOINTS_BUCKET=datapoints
SUPABASE_STIMULI_TABLE=stimuli

# Bayes CSV source from Supabase Storage
BAYES_PATIENT_ID=
# Optional override for storage object path. If empty, uses "<BAYES_PATIENT_ID>.csv"
BAYES_DATAPOINTS_OBJECT_PATH=

# Loss model backend: "cnn" or "xgboost"
LOSS_MODEL_BACKEND=cnn
# Optional override path for xgboost .json model
LOSS_XGBOOST_MODEL_PATH=
